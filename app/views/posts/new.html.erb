  <div class="container">
  <div id="form-content" >
    <h1>Share a picture of your work</h1>
  <%= simple_form_for [@workshop, @post] do |f| %>
    <%= f.input :title %>
    <%= f.input :photo, as: :file %>
    <%= f.submit "Make Post", class: "button-pink-devise" %>
  <% end %>
  </div>
</div>



